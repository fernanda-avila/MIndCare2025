# Exemplo .env para BackEnd
# Conexão com o banco (Postgres)
DATABASE_URL=postgresql://user:password@localhost:5432/mindcare

# Chave para provedor de IA (OpenAI ou outro). Se não definido, o backend usará mock.
OPENAI_API_KEY=
# Modelo para Google Generative (ex: text-bison-001)
GOOGLE_MODEL=text-bison-001

# Hugging Face (opcional) - se quiser usar HF como alternativa grátis
HUGGINGFACE_API_KEY=
HUGGINGFACE_MODEL=google/flan-t5-small
